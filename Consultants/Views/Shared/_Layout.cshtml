<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Consultants WebSite</title>
    <meta name="viewport" content="width=device-width" />

    <link rel="stylesheet" type="text/css" href="~/Content/bootstrap-responsive.css" />

    <meta charset='utf-8'>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <script src="http://code.jquery.com/jquery-latest.min.js" type="text/javascript"></script>
 
    <title>CSS MenuMaker</title>
    <link rel="stylesheet" type="text/javascript" href="~/Scripts/bootstrap.js" />
    <link rel="stylesheet" type="text/css" href="~/Content/bootstrap.css" />
    @Styles.Render("~/Content/bootstrap.min.css")
    @Styles.Render("~/Content/bootstrap-responsive.min.css")
    @Scripts.Render("~/bundles/jquery")
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>
    <script>
        $(function () {
           
            $('#signOutButton').click(function () {

                $.ajax({
                    url: "@Url.Action("Logout", "Account")",
                  

                success: function (response) {
          
                   
                    window.location.reload();
                }
                });

            });
        });
    </script>


</head>



<body>
    <header id="mynav">
        <ul>
        
            <li><a onclick="location.href='@Url.Action("Login","account")'" o class="active">Home</a></li>
           
             <li><a class="active" onclick="location.href='@Url.Action("SearchConsultants","account")'">Home Page</a></li>
           <li><a  class="active" onclick="location.href='@Url.Action("SmartProfile","account")'">SmartForm</a></li>
            <li><a  class="active" onclick="location.href='@Url.Action("ChangePassword","account")'">Change Password</a></li>
            <li class="rightfloat"><a  id="signOutButton" >Sign Out</a></li>
            
                </ul>
    </header>
    


    
    <div class="container">

        @RenderBody()
       
        <footer>
           
        </footer>
    </div>

 
   
    @RenderSection("scripts", required: false)

</body>
</html>
